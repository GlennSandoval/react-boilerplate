{
  "extends": [
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "babelOptions": {
      "plugins": [
        "@babel/plugin-proposal-class-properties"
      ]
    }
  },
  "plugins": [
    "@babel"
  ]
}
